        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "bsq/.prior_art/repos/*": true,
        "fillit/prior_art/*": true
    },
    "C_Cpp.default.includePath": ["fillit"]
}
